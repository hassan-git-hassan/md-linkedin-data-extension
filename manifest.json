{
    "manifest_version": 3,
    "name": "MD LinkedIn Profile Copier",
    "version": "2.0",
    "description": "Chrome extension to help you copy selected LinkedIn profile data.",
    "author": "Hassan Mehmood",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "host_permissions": [
      "https://www.linkedin.com/in/*"
    ]
  }
  